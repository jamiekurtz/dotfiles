if [ ! -f ~/.vimrc ]; then 
    ln -s ~/wd/dotfiles/.vimrc ~/.vimrc
fi

if [ ! -f ~/.gitconfig ]; then 
    ln -s ~/wd/dotfiles/.gitconfig ~/.gitconfig
fi

if [ ! -f ~/.bash_aliases ]; then 
    ln -s ~/wd/dotfiles/.bash_aliases ~/.bash_aliases
fi


mkdir -p ~/.vim
cp .vim/ ~/ -r
mkdir -p ~/.backup


