if [ ! -f ~/.vimrc ]; then 
    ln -s ~/wd/dotfiles/.vimrc ~/.vimrc
fi

if [ ! -f ~/.gitconfig ]; then 
    ln -s ~/wd/dotfiles/.gitconfig ~/.gitconfig
fi

if [ ! -f ~/.bashrc ]; then 
    ln -s ~/wd/dotfiles/.bashrc ~/.bashrc
fi


test -d ~/.vim || mkdir ~/.vim
cp .vim/ ~/.vim -r


test -d ~/.gnome2/gedit/plugins || mkdir ~/.gnome2/gedit/plugins
cp .gnome2/gedit/plugins/* ~/.gnome2/gedit/plugins/

