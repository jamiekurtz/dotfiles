if [ ! -f ~/.vimrc ]; then 
    ln -s ~/wd/dotfiles/.vimrc ~/.vimrc
fi

if [ ! -f ~/.gitconfig ]; then 
    ln -s ~/wd/dotfiles/.gitconfig ~/.gitconfig
fi

if [ ! -f ~/.bash_aliases ]; then 
    ln -s ~/wd/dotfiles/.bash_aliases ~/.bash_aliases
fi

if [ ! -f ~/.bashrc ]; then 
    ln -s ~/wd/dotfiles/.bashrc ~/.bashrc
fi


mkdir -p ~/.vim
cp -r .vim ~/ 
mkdir -p ~/.backup


