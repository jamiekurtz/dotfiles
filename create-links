if [ ! -f ~/.vimrc ]; then 
    ln -s ~/wd/dotfiles/.vimrc ~/.vimrc
fi

if [ ! -f ~/.gitconfig ]; then 
    ln -s ~/wd/dotfiles/.gitconfig ~/.gitconfig
fi

#if [ ! -f ~/.bashrc ]; then 
#    ln -s ~/wd/dotfiles/.bashrc ~/.bashrc
#fi


mkdir -p ~/.vim
cp .vim/ ~/ -r
mkdir ~/.backup


mkdir -p ~/.local/share/gedit/plugins
cp .local/share/gedit/plugins/* ~/.local/share/gedit/plugins -r

