if [ ! -f ~/.vimrc ]; then 
    ln -s ~/wd/dotfiles/.vimrc ~/.vimrc
fi

if [ ! -f ~/.gitconfig ]; then 
    ln -s ~/wd/dotfiles/.gitconfig ~/.gitconfig
fi

if [ ! -f ~/.bashrc ]; then 
    ln -s ~/wd/dotfiles/.bashrc ~/.bashrc
fi


mkdir ~/.vim
mkdir ~/.vim/colors
cp zenburn.vim ~/.vim/colors


cp xfce4-keyboard-shortcuts.xml ~/.config/xfce4/xfconf/xfce-perchannel-xml
